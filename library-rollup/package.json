{
  "type": "module",
  "name": "library-rollup",
  "version": "0.1.0",
  "description": "TypeScript library npm package boilerplate",
  "author": "Devin <89256006@qq.com>",
  "homepage": "https://github.com/adntin/library-demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/adntin/library-demo"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "npm",
    "package",
    "boilerplate"
  ],
  "main": "dist/library-rollup.cjs.js",
  "module": "dist/library-rollup.esm.js",
  "unpkg": "dist/library-rollup.umd.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "prebuild": "rimraf ./dist",
    "build": "rollup -c",
    "predocs": "rimraf ./docs",
    "docs": "typedoc src --out docs",
    "clean": "rimraf ./dist ./docs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "eslint": "8.46.0",
    "prettier": "3.0.1",
    "rollup": "3.27.0",
    "tslib": "2.6.1",
    "typedoc": "0.24.8",
    "typescript": "5.1.6"
  }
}
